<openerp><data>

    <record id="view_product_scale_log_wizard_form" model="ir.ui.view">
        <field name="model">product.scale.log.wizard</field>
        <field name="arch" type="xml">
        <form string="Mass Log to Bizerba" version="7.0">
            <group col="2">
                <field name="product_qty"/>
                <field name="log_type"/>
            </group>
            <footer>
                <button name="send_log" string="Send Log" type="object" class="oe_highlight"/>
                or
                <button string="Cancel" class="oe_link" special="cancel"/>
            </footer>
        </form>
        </field>
    </record>

</data></openerp>
